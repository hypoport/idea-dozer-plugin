<idea-plugin version="2">
  <id>de.hypoport.plugins.DozerPlugin</id>
  <name>Dozer</name>
  <version>2.0</version>
  <vendor url="http://www.hypoport.de">Hypoport AG</vendor>

  <description><![CDATA[Dozer support plugin]]></description>

  <change-notes><![CDATA[rework for IDEA 13]]></change-notes>

  <!-- please see http://confluence.jetbrains.com/display/IDEADEV/Build+Number+Ranges for description -->
  <idea-version since-build="133"/>

  <!-- please see http://confluence.jetbrains.com/display/IDEADEV/Plugin+Compatibility+with+IntelliJ+Platform+Products
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <standardResourceProvider implementation="de.hypoport.plugins.dozer.DozerResourceProvider" />
    <dom.fileDescription implementation="de.hypoport.plugins.dozer.DozerFileDescription" />
  </extensions>

  <application-components>
    <!-- Add your application components here -->
  </application-components>

  <project-components>
    <!-- Add your project components here -->
  </project-components>

  <actions>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>